    "# helper function to count freqency of words\n",
    "def count_freq(series):\n",
    "    cnt = Counter()\n",
    "    for doc in series:\n",
    "        for word in doc:\n",
    "            cnt[word]+=1\n",
    "    return cnt"
   ]
  },
